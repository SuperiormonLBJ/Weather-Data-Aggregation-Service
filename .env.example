# Required API Keys
OPENWEATHER_API_KEY=your_openweather_api_key_here
WEATHERAPI_KEY=your_weatherapi_key_here

# Cache Configuration
CACHE_TTL=600

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=logs/weather.log

# API Timeout Settings (seconds)
OPENWEATHER_TIMEOUT_TOTAL=7.0
OPENWEATHER_TIMEOUT_CONNECT=2.0
WEATHERAPI_TIMEOUT_TOTAL=7.0
WEATHERAPI_TIMEOUT_CONNECT=2.0
OPENMETEO_TIMEOUT_TOTAL=8.0
OPENMETEO_TIMEOUT_CONNECT=2.0

# Rate Limiting Settings
OPENWEATHER_TOKENS=60
OPENWEATHER_REFILL_RATE=1.0
WEATHERAPI_TOKENS=100
WEATHERAPI_REFILL_RATE=1.5
OPENMETEO_TOKENS=1000
OPENMETEO_REFILL_RATE=10.0

# Retry Configuration
MAX_RETRIES=3
BASE_DELAY=1.0
MAX_DELAY=16.0
JITTER_FACTOR=0.1
BACKOFF_MULTIPLIER=2.0

# Connection pool Configuration
CONNECTION_POOL_SIZE=100
CONNECTION_POOL_PER_HOST=30
CONNECTION_KEEPALIVE_TIMEOUT=30